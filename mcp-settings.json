{
  "mcpServers": {
    "awslabs.aws-documentation-mcp-server": {
      "command": "python",
      "args": [
        "-c", "from awslabs.aws_documentation_mcp_server.server import main; main()"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "AWS_DOCUMENTATION_PARTITION": "aws"
      },
      "disabled": false,
      "autoApprove": [
        "search_documentation",
        "read_documentation",
        "recommend"
      ]
    }
  }
}
